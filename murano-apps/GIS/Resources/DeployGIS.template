FormatVersion: 2.1.0
Version: 1.0.0
Name: Deploy GIS

Parameters:
  port: $port

Body: |
  return executeRecipe(args).stdout

Scripts:
  executeRecipe:
    Type: Chef
    Version: 1.0.0
    EntryPoint: GIS::4.4.3_install
    Files:
        - GIS : https://forge.fiware.org/scmrepos/svn/testbed/trunk/cookbooks/GESoftware/GIS

    Options:
      captureStdout: true
      captureStderr: true
