FormatVersion: 2.1.0
Version: 1.0.0
Name: Deploy repository

Parameters:
  port: $port


Body: |
  return executeRecipe(args).stdout

Scripts:
  executeRecipe:
    Type: Chef
    Version: 1.0.0
    EntryPoint: repository::default
    Files:
        - repository : https://forge.fiware.org/scmrepos/svn/testbed/trunk/cookbooks/GESoftware/repository

    Options:
      captureStdout: true
      captureStderr: true
