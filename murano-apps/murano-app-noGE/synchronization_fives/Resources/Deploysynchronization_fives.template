FormatVersion: 2.1.0
Version: 1.0.0
Name: Deploy synchronization_fives

Parameters:
  port: $port


Body: |
  return executeRecipe(args).stdout

Scripts:
  executeRecipe:
    Type: Chef
    Version: 1.0.0
    EntryPoint: synchronization_fives::default
    Files:
        - synchronization_fives : https://forge.fiware.org/scmrepos/svn/testbed/trunk/cookbooks/GESoftware/synch-fives/

    Options:
      captureStdout: true
      captureStderr: true
