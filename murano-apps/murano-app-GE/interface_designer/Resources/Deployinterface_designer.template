FormatVersion: 2.1.0
Version: 1.0.0
Name: Deploy interface_designer

Parameters:
  port: $port


Body: |
  return executeRecipe(args).stdout

Scripts:
  executeRecipe:
    Type: Chef
    Version: 1.0.0
    EntryPoint: interface_designer::4.4.3_install
    Files:
        - interface_designer : https://forge.fiware.org/scmrepos/svn/testbed/trunk/cookbooks/GESoftware/interface_designer

    Options:
      captureStdout: true
      captureStderr: true
